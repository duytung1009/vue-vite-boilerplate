{
  "name": "vue-vite-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite build && vite preview",
    "start": "yarn dev & wait-on tcp:3000 -v",
    "test": "cypress open",
    "test-components": "cypress open-ct",
    "test:ci": "cypress run --headless",
    "test:ci-components": "cypress run-ct"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,scss}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@vueuse/head": "~0.7",
    "vue": "~3.2",
    "vue-router": "~4.0",
    "vuex": "~4.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "~2.2",
    "@cypress/vue": "~3.0",
    "@tailwindcss/aspect-ratio": "~0.4",
    "@tailwindcss/forms": "~0.4",
    "@tailwindcss/line-clamp": "~0.3",
    "@tailwindcss/typography": "~0.5",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "~2",
    "@vue/eslint-config-standard": "~6.1",
    "@vue/eslint-config-typescript": "~10",
    "autoprefixer": "~10",
    "cypress": "~9.3",
    "eslint": "~8.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "~2",
    "eslint-plugin-node": "~11.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "~6.0",
    "eslint-plugin-vue": "~8",
    "postcss": "~8.4",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "~10",
    "prettier": "^2.5.1",
    "tailwindcss": "~3",
    "typescript": "^4.5.5",
    "vite": "~2.7",
    "wait-on": "~6.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3"
  }
}
